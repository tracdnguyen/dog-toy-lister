<h1>Login to Dog Toy Lister!</h1>
<% if flash.has?(:error) %>
  <%= flash[:error] %>
<% end %>
<form method="post" action="/login">
  <p>Username: <input type="text" name="username" required></br></p>
  <p>Password: <input type="password" name="password" required></p>

  <input type="submit" value="Submit">
</form>
